{
	"name": "thesearchpage",
	"description": "Provides a proper search page",
	"version": "1.1.1",
	"author": "Magnus Anderssen <magnus@magooweb.com>",
	"contributors": [],
	"bugs": {
		"url": "https://github.com/callmemagnus/nextcloud-searchpage/issues"
	},
	"license": "agpl",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "run-p dev:*",
		"build": "run-p build:*",
		"dev:app": "vite --config vite.app.config.ts build --mode dev",
		"build:app": "vite --config vite.app.config.ts build",
		"dev:admin": "vite --config vite.admin.config.ts --mode dev build",
		"build:admin": "vite --config vite.admin.config.ts build",
		"test": "vitest",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"lint": "eslint --config .eslintrc.cjs src/**/*.ts src/**/*.svelte"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "2.5.2",
		"@testing-library/jest-dom": "6.1.4",
		"@testing-library/svelte": "4.0.5",
		"@testing-library/user-event": "14.5.1",
		"@tsconfig/svelte": "5.0.2",
		"@typescript-eslint/eslint-plugin": "6.11.0",
		"@typescript-eslint/parser": "6.11.0",
		"@vitejs/plugin-vue2": "2.2.0",
		"@vitest/coverage-v8": "0.34.6",
		"@vitest/ui": "0.34.6",
		"autoprefixer": "10.4.16",
		"eslint": "8.53.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.1",
		"eslint-plugin-svelte": "2.35.0",
		"glob": "10.3.10",
		"less": "4.2.0",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.31",
		"postcss-load-config": "4.0.1",
		"prettier": "3.1.0",
		"prettier-plugin-svelte": "3.1.0",
		"svelte": "4.2.3",
		"svelte-check": "3.6.0",
		"tailwindcss": "3.3.5",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"vite": "4.5.0",
		"vite-plugin-css-injected-by-js": "3.3.0",
		"vite-plugin-html": "3.2.0",
		"vitest": "0.34.6"
	},
	"dependencies": {
		"@nextcloud/axios": "2.4.0",
		"@nextcloud/l10n": "2.2.0",
		"@nextcloud/password-confirmation": "5.0.0",
		"@nextcloud/router": "2.2.0",
		"@nextcloud/typings": "1.7.0"
	}
