{
  "name": "thesearchpage",
  "description": "Provides a proper search page",
  "version": "1.1.1",
  "author": "Magnus Anderssen <magnus@magooweb.com>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/callmemagnus/nextcloud-searchpage/issues"
  },
  "license": "agpl",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "build": "run-p build:*",
    "dev:app": "vite --config vite.app.config.ts build --mode dev",
    "build:app": "vite --config vite.app.config.ts build",
    "dev:admin": "vite --config vite.admin.config.ts --mode dev build",
    "build:admin": "vite --config vite.admin.config.ts build",
    "test": "vitest",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint --config .eslintrc.cjs src/**/*.ts src/**/*.svelte",
    "doctor": "npm run lint && npm run build && npx playwright test"
  },
  "devDependencies": {
    "@playwright/test": "1.43.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/svelte": "5.1.0",
    "@testing-library/user-event": "14.5.2",
    "@tsconfig/svelte": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "@vitejs/plugin-vue2": "2.3.1",
    "@vitest/coverage-v8": "1.5.1",
    "@vitest/ui": "1.5.1",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-svelte": "2.38.0",
    "glob": "10.3.12",
    "jsdom": "^24.0.0",
    "less": "4.2.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.38",
    "postcss-load-config": "5.0.3",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.3",
    "svelte": "4.2.15",
    "svelte-check": "3.6.9",
    "tailwindcss": "^3.4.3",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vite-plugin-css-injected-by-js": "3.5.0",
    "vite-plugin-html": "3.2.2",
    "vitest": "1.5.1"
  },
  "dependencies": {
    "@nextcloud/axios": "2.4.0",
    "@nextcloud/browserslist-config": "3.0.1",
    "@nextcloud/l10n": "2.2.0",
    "@nextcloud/password-confirmation": "5.1.0",
    "@nextcloud/router": "3.0.1",
    "@nextcloud/typings": "1.8.0"
